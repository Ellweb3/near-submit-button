{
  "displayName": "View Method",
  "defaultWidth": 554,
  "defaultHeight": 128,
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "title",
      "displayName": "Button Title",
      "type": "text",
      "default": "View",
      "styles": {
        "fontFamily": "@body",
        "color": "@contrast:backgroundColor",
        "fontWeight": "normal",
        "borderRadius": "18px"
      }
    },
    {
      "name": "viewMethodName",
      "displayName": "View method Name",
      "type": "text",
      "default": "getAllNFTsByOwner"
    },
    {
      "name": "collection",
      "displayName": "What the collecton we need to fill?",
      "type": "list"
    },
    {
      "name": "compareId",
      "displayName": "compareId",
      "type": "text",
      "role": "listItem",
      "reference": "collection"
    },
    {
      "name": "parametr1Name",
      "displayName": "parametr1Name",
      "type": "text"
    },
    {
      "name": "parametr1Value",
      "displayName": "parametr1Value",
      "type": "text"
    },
    {
      "name": "parametr2Name",
      "displayName": "parametr2Name",
      "type": "text"
    },
    {
      "name": "parametr2Value",
      "displayName": "parametr2Value",
      "type": "text"
    },
    {
      "name": "parametr3Name",
      "displayName": "parametr3Name",
      "type": "text"
    },
    {
      "name": "parametr3Value",
      "displayName": "parametr3Value",
      "type": "text"
    },
    {
      "name": "contractName",
      "displayName": "Contract Name",
      "type": "text",
      "default": "one.nfts.testnet"
    },
    {
      "name": "netType",
      "displayName": "Net Type",
      "type": "text",
      "default": "testnet",
      "control": {
        "type": "menu",
        "options": [
          {
            "label": "TestNet",
            "value": "testnet"
          },
          {
            "label": "MainNet",
            "value": "mainnet"
          }
        ]
      }
    },
    {
      "name": "onView",
      "displayName": "Trigger when we get result from calling contract method",
      "type": "action",
      "arguments": [
        {
          "type": "number",
          "displayName": "Number value"
        },
        {
          "type": "text",
          "displayName": "Text value"
        },
        {
          "type": "text",
          "displayName": "Text value2"
        }
      ]
    }
  ]
}